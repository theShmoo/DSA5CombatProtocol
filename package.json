{
  "name": "DSA5WebSpellBook",
  "description": "A DSA 5 combat protocol utility",
  "author": "David Pfahler",
  "version": "1.0.0",
  "homepage": ".",
  "repository": {
    "type": "git",
    "url": "https://github.com/theShmoo/DSA5WebSpellBook.git"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/styles/ -o src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/ -o src/styles/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "bootstrap": "^3.4.1",
    "node-sass-chokidar": "^1.2.2",
    "react": "^16.2.0",
    "react-autosuggest": "^9.0.0",
    "react-bootstrap": "^0.32.0",
    "react-bootstrap-autosuggest": "^0.4.1",
    "react-dnd": "^2.2.4",
    "react-dnd-html5-backend": "^2.2.4",
    "react-dom": "^16.2.0",
    "react-images": "^0.5.15",
    "react-scripts": "1.1.0",
    "react-select": "^1.2.1"
  }
}
